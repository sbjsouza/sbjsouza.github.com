@article{Barros2009,
abstract = {In this paper, we propose a hybrid machine learning approach to Information Extraction by combining conventional text classification techniques and Hidden Markov Models (HMM). A text classifier generates a (locally optimal) initial output, which is refined by an HMM, providing a globally optimal classification. The proposed approach was evaluated in two case studies and the experiments revealed a consistent gain in performance through the use of the HMM. In the first case study, the implemented prototype was used to extract information from bibliographic references, reaching a precision rate of 87.48{\%} in a test set with 3000 references. In the second case study, the prototype extracted information from author affiliations, reaching a precision rate of 90.27{\%} in a test set with 300 affiliations. Read More: http://www.worldscientific.com/doi/abs/10.1142/S0218213009000147},
author = {Barros, Flavia Almeida and Silva, Eduardo F A and Prudêncio, Ricardo B.C. and Filho, Valmir M. and Nascimento, Andre C.A.},
file = {:home/andrecamara/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Barros et al. - 2009 - Combining Text Classifiers and Hidden Markov Models for Information Extraction.pdf:pdf},
journal = {International Journal on Artificial Intelligence Tools},
keywords = {hmm,information extraction,text classifiers},
number = {2},
pages = {311--329},
title = {{Combining Text Classifiers and Hidden Markov Models for Information Extraction}},
